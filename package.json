{
  "name": "todo-er",
  "version": "1.0.0",
  "description": "A simple todo demo for data-modela npm package",
  "main": "index.js",
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>setup/setupEnzyme.js"
    ]
  },
  "scripts": {
    "webpack": "webpack",
    "test": "jest",
    "testserver": "mocha -r esm src/test/index.js",
